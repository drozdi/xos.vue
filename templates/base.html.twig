<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <meta http-equiv="cache-control" content="no-cache, no-store, must-revalidate" />
        <meta http-equiv="pragma" content="no-cache" />
        <meta http-equiv="expires" content="Fri, 20 Mar 2014 00:00:00 GMT" />

        <title>{% block title %}Welcome!{% endblock %}</title>
        <link rel="icon" href="data:image/svg+xml,<svg xmlns=%22http://www.w3.org/2000/svg%22 viewBox=%220 0 128 128%22><text y=%221.2em%22 font-size=%2296%22>⚫️</text><text y=%221.3em%22 x=%220.2em%22 font-size=%2276%22 fill=%22%23fff%22>sf</text></svg>">
        {% block stylesheets %}
            {{ encore_entry_link_tags('app') }}
        {% endblock %}
        <script>
            window.__QUASAR_SSR_SERVER__ = false
            window.__VUE_OPTIONS_API__ = true
            window.__VUE_PROD_DEVTOOLS__ = false
            window.__VUE_PROD_HYDRATION_MISMATCH_DETAILS__ = false
            function s() {
                if (!performance.memory) return
                console.log('jsHeapSizeLimit', (performance.memory.jsHeapSizeLimit/8/1024/1024) + 'Mb')
                console.log('totalJSHeapSize', (performance.memory.totalJSHeapSize/8/1024/1024) + 'Mb')
                console.log('usedJSHeapSize', (performance.memory.usedJSHeapSize/8/1024/1024) + 'Mb')
                setTimeout(s, 5000)
            }
            s()
        </script>
        {% block javascripts %}
            {{ encore_entry_script_tags('app') }}
        {% endblock %}
    </head>
    <body>
		<div id="q-app" style="width: 100%; height: 100%;"></div>
    </body>
</html>
